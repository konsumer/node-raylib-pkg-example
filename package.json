{
  "name": "node-raylib-pkg-example",
  "version": "0.0.0",
  "private": true,
  "author": "konsumer",
  "license": "MIT",
  "scripts": {
    "build:windows": "pkg -t node18-win .",
    "build:macos": "pkg -t node18-macos .",
    "build:linux": "pkg -t node18-linux .",
    "build:bun": "bun build --compile --outfile=dist/node-raylib-pkg-example-bun ./test.js",
    "start": "node test.js"
  },
  "pkg": {
    "assets": ["node_modules/**/*.node"],
    "outputPath": "dist"
  },
  "dependencies": {
    "raylib": "^0.14.0"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^6.6.0"
  },
  "bin": {
    "node-raylib-pkg-example": "test.js"
  }
}
